<div class="container">
	<h1>List of Users</h1>
	<table class="table">
	<th>ID</th><th>Name</th><th>Email</th><th>Full Name</th><th>Password</th><th>Token</th><th>Verified</th>
	<tbody>
	<% @users.each do |user| %>
	    <tr>
			<td><%= user.id %></td><td><%= user.username %></td><td><%= user.email %></td>
			<td><%= user.first_name+" "+ user.last_name %></td><td><%= user.password %></td>
			<td><%= user.token %></td><td><%= user.verified %></td>		
		</tr>
	<% end %>
    </tbody>  
   </table>	
</div>